{
  "name": "@jict/changelogithub",
  "type": "module",
  "version": "0.0.16",
  "packageManager": "pnpm@7.29.1",
  "description": "Generate changelog for GitHub.",
  "keywords": [
    "github",
    "release",
    "releases",
    "conventional",
    "changelog",
    "log"
  ],
  "license": "MIT",
  "author": "justInCase, Inc. <developers@justincase.jp>",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justincase-jp/changelogithub.git"
  },
  "homepage": "https://github.com/justincase-jp/changelogithub#readme",
  "bugs": "https://github.com/justincase-jp/changelogithub/issues",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "*.mjs"
  ],
  "bin": "./cli.mjs",
  "sideEffects": false,
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "release": "bumpp --commit --push --tag",
    "start": "pnpm dev && node cli.mjs",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@antfu/utils": "^0.7.2",
    "c12": "^1.2.0",
    "cac": "^6.7.14",
    "changelogen": "0.5.1",
    "convert-gitmoji": "0.1.3",
    "execa": "^7.1.1",
    "kolorist": "^1.7.0",
    "octokit": "^3.1.2",
    "ohmyfetch": "^0.4.21"
  },
  "devDependencies": {
    "@hannoeru/eslint-config": "^0.9.4",
    "@types/conventional-changelog": "^3.1.1",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^11.0.1",
    "@types/minimist": "^1.2.2",
    "@types/semver": "^7.3.13",
    "bumpp": "^9.0.0",
    "eslint": "^8.36.0",
    "fs-extra": "^11.1.0",
    "typescript": "^4.9.5",
    "unbuild": "^1.1.2",
    "vitest": "^0.29.2"
  }
}
